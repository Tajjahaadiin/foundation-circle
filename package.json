    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.4.3",
    "msw": "^2.7.0",
    "prettier": "^3.4.2",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.23.0",
